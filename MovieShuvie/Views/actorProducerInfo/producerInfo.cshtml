
@{
    ViewBag.Title = "producerInfo";
}

@{
    MovieShuvie.Models.producers prod = (MovieShuvie.Models.producers)
        ViewData["producer"];
}

<h2>producerInfo</h2>

<div class="row">
    <div class="col-md-6">
        <div class="well">
            Name: <br />
            <p class="bg-info">@prod.producerName</p>
            Sex:<br />
            <p class="bg-info">@prod.producerSex</p>
            DOB:<br />
            <p class="bg-info">@prod.producerDOB</p>
            Bio:<br />
            <p class="bg-info">@prod.producerInfo</p>
        </div>
    </div>
</div>


